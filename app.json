{
  "expo": {
    "name": "framez-app",
    "slug": "framez-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.stace.framezapp"
    },
    "android": {
      "package": "com.stace.framezapp",
      "permissions": ["android.permission.RECORD_AUDIO"],
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      }
    },
    "web": {},
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "55fd15d4-7f8c-469a-ab36-55fd9c4364d0"
      }
    }
  }
}
